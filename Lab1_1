   "id": "ac3d9e96-6011-48d4-9c9e-849590ff0a01",
   "metadata": {},
   "outputs": [],
   "source": [
    "class Model(object):\n",
    "    \"\"\"Модель парной линейной регрессии\"\"\"\n",
    "    def __init__(self):\n",
    "        self.b0 = 1\n",
    "        self.b1 = 2\n",
    "    def predict(self, x):\n",
    "        return self.b0 + self.b1 * x\n",
    "    def error(self, X, Y):    \n",
    "        return sum((self.predict(X) - Y)**2) / (2 * len(Y))\n",
    "    def fit(self, X, Y):  \n",
    "        alpha = 0.1\n",
    "        dJ0 = sum(self.predict(X) - Y) /len(Y)\n",
    "        dJ1 = sum((self.predict(X) - Y) * X) /len(Y)\n",
    "        self.b0 -= alpha * dJ0\n",
    "        self.b1 -= alpha * dJ1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 355,
   "id": "953b41ac-ab26-44de-aca8-e113d1073f77",
